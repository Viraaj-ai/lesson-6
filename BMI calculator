height=float(input("enter your height in cm:"))
weight=float(input("enter your weight in kgs:"))
BMI=weight/(height/100)**2
print(BMI)
if BMI<=18.5:
    print("You are underweight")
elif BMI<=24.9:
    print("You are healthy")
elif BMI<=29.9:
    print("You are over weight")
else:
    print("You are obese")

